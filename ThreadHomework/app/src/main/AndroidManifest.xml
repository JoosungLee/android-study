<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="joosung.threadhomework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity1"></activity>
        <activity android:name=".Activity2"></activity>
        <activity android:name=".Activity3"></activity>
        <activity android:name=".Activity4"></activity>
        <activity android:name=".ImplicitIntentActivity"></activity>
        <activity android:name=".ServiceActivity"></activity>

        <service android:name=".ServiceClass">
            <intent-filter>
                <action android:name="joosung.threadhomework"/>
            </intent-filter>
        </service>

    </application>

</manifest>
